<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:orientation="vertical" 
   
    android:layout_width="fill_parent" 
    android:layout_height="fill_parent"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <RelativeLayout 
        android:id="@id/rel_tou" 
        android:layout_width="fill_parent" 
        android:layout_height="50.0dip">
        <ImageView 
            android:id="@+id/setting_back"
             android:layout_width="wrap_content"
              android:layout_height="wrap_content"
               android:layout_marginLeft="3.0dip" 
               android:src="@drawable/back" 
               android:layout_alignParentLeft="true"
                android:layout_centerVertical="true" 
               />
        <TextView
             android:textSize="16sp" 
             android:gravity="center_vertical" 
             android:id="@id/textview_title" 
             android:layout_width="wrap_content" 
             android:layout_height="wrap_content" 
             android:text="设置"
              android:layout_centerHorizontal="true"
               android:layout_centerVertical="true" />
    </RelativeLayout>
    <ImageView 
        android:id="@id/iv_xian"
         android:layout_width="fill_parent" 
         android:layout_height="wrap_content"
          android:src="@drawable/logo_xian" 
          android:scaleType="fitXY" />
    <LinearLayout 
        android:orientation="vertical" 
        android:background="@drawable/setting_first_bg"
         android:layout_width="fill_parent" 
         android:layout_height="wrap_content" 
         android:layout_marginTop="15.0dip">
        <RelativeLayout 
            android:id="@+id/setting_rel_cleanHuancun"
             android:layout_width="fill_parent" 
             android:layout_height="wrap_content"
            >
            <ImageView 
                android:id="@+id/imageView_icon" 
                android:visibility="invisible" 
                android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                  android:layout_marginLeft="15.0dip" 
                  android:layout_marginTop="9.0dip" 
                  android:layout_marginBottom="9.0dip" 
                  android:src="@drawable/myself_youhuiquan" 
                  android:layout_toRightOf="@+id/textView_text"
                   android:layout_centerVertical="true" />
            <ImageView 
                android:id="@+id/imageView_jiantou" 
                android:layout_width="wrap_content"
                 android:layout_height="wrap_content" 
                 android:layout_marginRight="15.0dip" 
                 android:src="@drawable/jiantou" 
                 android:layout_alignParentRight="true" 
                 android:layout_centerVertical="true" />
            <TextView 
                android:textSize="16.0sp" 
                android:id="@+id/textView_text" 
                 android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                   android:layout_marginLeft="18.0dip" 
                   android:text="清除缓存" 
                   android:maxEms="8"
                    android:layout_alignParentLeft="true"
                     android:layout_centerVertical="true" />
            <TextView 
                android:textSize="16.0sp" 
                android:id="@+id/tv_cacheSize" 
                android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:layout_marginRight="8.0dip"
                 
                 android:layout_toLeftOf="@id/imageView_jiantou" 
                 android:layout_centerVertical="true" />
        </RelativeLayout>
        <ImageView 
            android:background="@drawable/logo_xian" 
            android:layout_width="fill_parent" 
            android:layout_height="wrap_content"
             android:layout_marginLeft="10.0dip" />
        <RelativeLayout 
            android:id="@+id/setting_rel_kefudianhua"
            android:layout_width="fill_parent" 
             android:layout_height="wrap_content" 
             >
            <ImageView 
                android:id="@id/imageView_icon" 
                android:visibility="invisible" 
                android:layout_width="wrap_content"
                 android:layout_height="wrap_content" 
                 android:layout_marginLeft="15.0dip"
                  android:layout_marginTop="9.0dip" 
                  android:layout_marginBottom="9.0dip"
                   android:src="@drawable/myself_address"
                    android:layout_toRightOf="@id/textView_text" 
                    android:layout_centerVertical="true" />
            <ImageView 
                android:id="@id/imageView_jiantou" 
                android:layout_width="wrap_content" 
                android:layout_height="wrap_content"
                 android:layout_marginRight="15.0dip"
                  android:src="@drawable/jiantou"
                   android:layout_alignParentRight="true" 
                   android:layout_centerVertical="true" />
            <TextView 
                android:textSize="16.0sp" 
             
                 android:id="@id/textView_text" 
                 android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                   android:layout_marginLeft="18.0dip" 
                   android:text="客服电话" 
                   android:maxEms="8" 
                   android:layout_alignParentLeft="true" 
                   android:layout_centerVertical="true" />
            <TextView 
                android:textSize="16.0sp"
                 android:textColor="#ff0000"
                  android:id="@id/textView_text" 
                  android:layout_width="wrap_content" 
                  android:layout_height="wrap_content"
                   android:layout_marginRight="8.0dip" 
                   android:text="4009696876" 
                   android:layout_toLeftOf="@id/imageView_jiantou"
                    android:layout_centerVertical="true" />
        </RelativeLayout>
        <ImageView 
            android:background="@drawable/logo_xian" 
            android:layout_width="fill_parent" 
            android:layout_height="wrap_content"
             android:layout_marginLeft="10.0dip" />
        <RelativeLayout 
            android:id="@+id/setting_rel_banbengengxin"
            android:layout_width="fill_parent" 
            android:layout_height="wrap_content" 
             >
            <ImageView 
                android:id="@id/imageView_icon"
                 android:visibility="invisible" 
                 android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                   android:layout_marginLeft="15.0dip"
                    android:layout_marginTop="9.0dip" 
                    android:layout_marginBottom="9.0dip"
                     android:src="@drawable/myself_youhuiquan"
                      android:layout_toRightOf="@id/textView_text" 
                      android:layout_centerVertical="true" />
            <ImageView 
                android:id="@id/imageView_jiantou" 
                android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                  android:layout_marginRight="15.0dip" 
                  android:src="@drawable/jiantou" 
                  android:layout_alignParentRight="true" 
                  android:layout_centerVertical="true" />
            <TextView 
                android:textSize="16.0sp" 
                android:id="@id/textView_text"
                 android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                   android:layout_marginLeft="18.0dip" 
                   android:text="版本更新" 
                   android:maxEms="8"
                    android:layout_alignParentLeft="true"
                     android:layout_centerVertical="true" />
            <TextView 
                 android:textSize="16.0sp"
                 android:id="@+id/setting_banben" 
                 android:layout_width="wrap_content" 
                 android:layout_height="wrap_content"
                 android:layout_marginRight="8.0dip" 
                 android:text="3.0.1" 
                 android:layout_toLeftOf="@id/imageView_jiantou" 
                 android:layout_centerVertical="true" />
        </RelativeLayout>
        </LinearLayout>


               
               
                  
    
    
    
               <Button 
                    android:id="@+id/exist"
                    android:layout_marginTop="40dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/address_delete_bg"
                    android:text="退出登录"
                    android:textColor="#ffffff"
                    />








</LinearLayout>